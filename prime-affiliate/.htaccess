Options -Indexes
Options +FollowSymLinks
Options -MultiViews
DirectoryIndex index.php
RewriteEngine on

RewriteRule ^we-welcome-you/?$ index.php [NC]
RewriteRule ^login/?$ login.php [NC]
RewriteRule ^signup/?$ signup.php [NC]
RewriteRule ^register/?$ sign-up.php [NC]
RewriteRule ^forgot-password/?$ forgot-password.php [NC]
RewriteRule ^end-current-session/?$ logout.php [NC]
RewriteRule ^_gt=forget-password/?$ forgetpassword.php [NC]

#ErrorDocument 404 /404.php
#ErrorDocument 500 /index.php/500/
#RewriteRule ^(.*)$ admin_login.php?/$1 [NC,L,QSA]
#RewriteRule ^(.*)$ admin_login.php/$1 [NC,L,QSA]